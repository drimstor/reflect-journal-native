{
  "expo": {
    "name": "Reflexity",
    "slug": "reflexity",
    "scheme": "reflexity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.reflexity"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#F1F2F3"
      },
      "package": "com.anonymous.reflexity"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#F1F2F3",
          "image": "./assets/images/logo.png",
          "dark": {
            "image": "./assets/images/logo.png",
            "backgroundColor": "#181822"
          },
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "dc34c5dd-1225-458c-b831-70832ef91cda"
      }
    }
  }
}
